{
    "files.autoSave": "afterDelay",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "git.autofetch": true,
    "files.associations": {
        "*.rmd": "markdown"
    },
    "[r]": {
        
    },
    "python.jediEnabled": false,
    "terminal.external.linuxExec": "urxvt",
    "r.rpath.lsp": "~/R/x86_64-pc-linux-gnu-library/3.5/languageserver",
    "r.linting.enable": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue"
}